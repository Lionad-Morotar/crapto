{
  "name": "file-text-crypto",
  "displayName": "file-text-crypto",
  "description": "crypto text which inside selected files or crypto entire file",
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "keywords": [
    "file",
    "crypto",
    "text",
    "aes",
    "partial"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "build": "node ./build/index.mjs",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "anymatch": "^3.1.2",
    "crypto-js": "^4.1.1",
    "rimraf": "^3.0.2"
  },
  "contributes": {
    "snippets": [
      {
        "language": "markdown",
        "path": "./snippets/snippets.json"
      }
    ],
    "commands": [
      {
        "title": "FTC: Decrypt the selected text.",
        "command": "ftc.decrypto"
      },
      {
        "title": "FTC: Enter the text before encryption.",
        "command": "ftc.prompt"
      }
    ],
    "keybindings": [
      {
        "command": "ftc.prompt",
        "key": "alt+f i"
      },
      {
        "command": "ftc.decrypto",
        "key": "alt+f d"
      }
    ]
  }
}
